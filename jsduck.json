{
	"--title": "TroopJS API Documentation",
	"--": [
		"bower_components/troopjs-core",
		"bower_components/troopjs-data",
		"bower_components/troopjs-browser",
		"bower_components/troopjs-utils",
		"bower_components/troopjs-jquery",
		"bower_components/troopjs-requirejs"
	],
	"--exclude": [
		"bower_components/troopjs-core/test",
		"bower_components/troopjs-data/test",
		"bower_components/troopjs-utils/test",
		"bower_components/troopjs-browser/test"
	],
	"--warnings": [
