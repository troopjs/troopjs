[
  {
    "name": "Framework",
    "groups": [
      {
        "name": "troopjs-compose",
        "classes": [ "compose.*" ]
      },
      {
        "name": "troopjs-core",
        "classes": [ "core.*" ]
      },
      {
        "name": "troopjs-hub",
        "classes": [
          "hub.emitter",
          "hub.component"
        ]
      },
      {
        "name": "troopjs-dom",
        "classes": [ "dom.*" ]
      }
    ]
  },
  {
    "name": "Stack",
    "groups": [
      {
        "name": "troopjs-ajax",
        "classes": [ "ajax.*" ]
      },
      {
        "name": "troopjs-hub-proxy",
        "classes": [ "hub.proxy.*" ]
      },
      {
        "name": "troopjs-widget",
        "classes": [ "widget.*" ]
      },
      {
        "name": "troopjs-l10n",
        "classes": [ "l10n.*" ]
      },
      {
        "name": "troopjs-route-hash",
        "classes": [ "route.hash.*" ]
      }
    ]
  },
  {
    "name": "Other",
    "groups": [
      {
        "name": "ECMAScript",
        "classes": [
          "RegExp",
          "Boolean",
          "Object",
          "String",
          "Date",
          "Function",
          "Array",
          "Number",
          "Promise"
        ]
      }
    ]
  }
]